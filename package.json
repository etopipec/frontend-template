{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build:prod": "nuxt build --dotenv .env.prod",
    "build:test": "nuxt build --dotenv .env.test",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint"
    ],
    "*.{ts,vue}": [
      "vue-tsc --noEmit"
    ]
  },
  "dependencies": {
    "@nuxt/eslint": "1.9.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/image": "1.11.0",
    "@nuxt/test-utils": "3.19.2",
    "@primeuix/themes": "1.2.3",
    "@primevue/nuxt-module": "4.3.9",
    "eslint": "9.35.0",
    "nuxt": "4.1.1",
    "vue": "3.5.21",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "6.14.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "typescript": "5.9.2"
  }
}
